{
  "name": "Like Tabs",
  "version": "1.0",
  "description": "Like Tabs!",
  "permissions": ["tabs","activeTab", "declarativeContent", "storage","contextMenus"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {                    
      "16": "images/LikeTabsLogo.png",
      "32": "images/LikeTabsLogo.png",
      "48": "images/LikeTabsLogo.png"
    },
    "default_title": "Like Tabs",     
    "default_popup": "popup.html"       
  },
  
  "icons": {
    "16": "images/LikeTabsLogo.png",
    "32": "images/LikeTabsLogo.png",
    "48": "images/LikeTabsLogo.png",
    "128": "images/LikeTabsLogo.png"
  },
  "manifest_version": 2
}
